{
	"name": "script-explorer",
	"displayName": "Scripts Explorer",
	"description": "Easily run your scripts using interface with buttons",
	"version": "0.0.1",
	"publisher": "mykytabalabuiev",
	"engines": {
		"vscode": "^1.69.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onView:script-explorer"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "script-explorer-view.refresh",
				"title": "Refresh script explorer"
			}
		],
		"views": {
			"script-explorer": [
				{
					"id": "script-explorer",
					"name": "Scripts Explorer"
				}
			]
		},
		"explorer": [
			{
				"id": "se-script",
				"name": "Scripts"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "script-explorer",
					"title": "Script Explorer",
					"icon": "resources/activity-bar-logo-2.svg"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.69.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.1",
		"@types/node": "16.x",
		"@typescript-eslint/eslint-plugin": "^5.30.0",
		"@typescript-eslint/parser": "^5.30.0",
		"eslint": "^8.18.0",
		"glob": "^8.0.3",
		"mocha": "^10.0.0",
		"typescript": "^4.7.4",
		"@vscode/test-electron": "^2.1.5"
	},
	"tasks-view": {
		"path": "C:\\cave\\projects\\tasks-window\\.vscode\\tasks.json"
	}
}